{
  "apps": [{
    "name": "app-dev",
    "cwd": "./",
    "script": "index.js",
    "watch": false,
    "exec_mode": "fork",
    "merge_logs": true,
    "log_date_format": " ",
    "out_file": "logs/out.log",
    "error_file": "logs/err.log",
    "autorestart": true,
    "max_memory_restart": "1G",
    "env": {
      "NODE_ENV": "development"
    }
  }]
}
